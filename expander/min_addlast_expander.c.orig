#include "minishell.h"

int	min_addlast_expander(t_expander **word, char *value, char *wo)
{
	t_expander	*end;

	end = min_last_expander(*word);
	if (wo && *wo)
	{
		end->key = l_word;
		end->word = value;
		*wo = 0;
	}
	else
	{
		end->next = min_init_expander(l_word, value);
		if (!(*word)->next)
		{
			min_free_expander(word);
			return (1);
		}
	}
	return (0);
}
